<%= form_with(model: [ @item, @item.associations.build ]) do |form| %>
    <div class="input-field">
      <%= form.label :name %><br>
      <%= form.text_field :name, :maxlength => 15 , :minlength => 3 %>
    </div>
    <div class="input-field">
      <%= form.label :ranking %><br>
      <%= form.number_field :ranking, :min => 0, :max => 100 %>
    </div>
    <p>
      <%= form.submit :class => "btn waves-effect waves-light" %>
    </p>
<% end %>